{
    "manifest_version": 2,
    "name": "Clear and Reload",
    "description": "Clear Cookie for current tab and reload",
    "version": "1",
    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Clear and Reload"
    },
    "author": {
        "name": "Chirag",
        "email": "b.like.no.other@gmail.com"
    }
}